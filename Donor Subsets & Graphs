ggplot(Donor2, aes(x=Score.3)) + geom_bar() + labs(title = "Donor 2, Score 3", x = "Score", y = "Frequency of Score")
ggplot(Donor2, aes(x=Score.4)) + geom_bar() + labs(title = "Donor 2, Score 4", x = "Score", y = "Frequency of Score")

