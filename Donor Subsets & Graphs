#Donor vs Mean Score SatterPlot
ggplot(printratings_dat, aes(x=donor, y=mean.score)) + geom_point()

#Donor vs Mean Score BarGraph
ggplot(printratings_dat, aes(x=donor, y=mean.score)) + geom_bar(stat="identity")

#Donor5 Subset and Graphs
Donor5 <- subset(printratings_dat, printratings_dat$donor == "5")
ggplot(Donor5, aes(x=Score.1)) + geom_bar() + labs(title = "Donor 5, Score 1", x = "Score", y = "Frequency of Score")
ggplot(Donor5, aes(x=Score.2)) + geom_bar() + labs(title = "Donor 5, Score 2", x = "Score", y = "Frequency of Score")
ggplot(Donor5, aes(x=Score.3)) + geom_bar() + labs(title = "Donor 5, Score 3", x = "Score", y = "Frequency of Score")
ggplot(Donor5, aes(x=Score.4)) + geom_bar() + labs(title = "Donor 5, Score 4", x = "Score", y = "Frequency of Score")

